{
    "compilerOptions": {
        "target": "es5",
        "alwaysStrict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "sourceMap": false,
        "noUnusedLocals": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        // "downlevelIteration": true,
        "lib": [ "es6", "dom", "esnext.asynciterable", "es2016" ],
        "outDir": "build/src",
    },
    "include": [ "src/**/*" ],
}